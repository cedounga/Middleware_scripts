                echo 'build'
                sleep 5
            }
        }
        stage('cedou') {
            steps {
                echo 'cedou'
                sleep 5
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy'
                sleep 5
            }
        }
    }
}
